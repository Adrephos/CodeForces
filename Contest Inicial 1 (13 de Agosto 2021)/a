#include <bits/stdc++.h>
using namespace std;
 
 
int main() {
    int n;
    cin>>n;
    
    vector<int> vec;
    int temp;
    for(int i = 0; i < n; i++){
        cin>>temp;
        vec.push_back(temp);
    }
    
    bool printed = false;
    for(int i = 0; i < vec.size(); i++){
        if(vec[i] == 1){
            cout<<"HARD"<<endl;
            printed = true;
            break;
        }
    }
    if(!printed){
        cout<<"EASY"<<endl;
    }
    
    return 0;
}
